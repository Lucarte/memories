{
    "version": 2,
    "builds": [
        {
            "src": "resources/js/*",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "public/build"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/public/build/$1"
        }
    ]
}
